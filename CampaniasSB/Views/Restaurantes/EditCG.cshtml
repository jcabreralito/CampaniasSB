            <label class="active" for="Restaurante">Restaurante</label>
        </div>
    </div>

    <div class="row">
        <div class="form-group focus col-sm-6" style="text-transform: uppercase !important;">
            <label for="TipoId">Tipo</label>
            <div class="customselect">
                @Html.DropDownList("TipoId", null, ((IList<ModelMetadata>)ViewData.ModelMetadata.Properties)[7].Watermark, htmlAttributes: new { @class = "form-control form-control-select clgfocus", @type = "text", @required = "true", style = "text-transform: uppercase", @oninvalid = "setCustomValidity('Seleccionar un elemento de la lista')", @oninput = "setCustomValidity('')" })
            </div>
            <span class="focus-border"></span>
        </div>
        <div class="form-group focus col-sm-6" style="text-transform: uppercase !important">
            <label for="NuevoNivelDePrecioId">Nivel De Precio</label>
            <div class="customselect">
                @Html.DropDownList("NuevoNivelDePrecioId", null, ((IList<ModelMetadata>)ViewData.ModelMetadata.Properties)[8].Watermark, htmlAttributes: new { @class = "form-control form-control-select clgfocus", @type = "text", @required = "true", style = "text-transform: uppercase", @oninvalid = "setCustomValidity('Seleccionar un elemento de la lista')", @oninput = "setCustomValidity('')" })
            </div>
            <span class="focus-border"></span>
        </div>
    </div>

    <div class="row">

        <div class="input-field center-align col-sm-6">
            @Html.CheckBoxFor(model => model.MenuDigital, new { @class = "switch_1" })
            <label class="active" for="MenuDigital">Menú Digital</label>
        </div>

        <div class="input-field col-sm-6" style="padding-right: 10px;">
            @Html.EditorFor(model => model.CantidadDePantallas, new { htmlAttributes = new { @type = "text", @class = "validate center-align", style = "text-transform: uppercase !important" } })
            <label class="active" for="CantidadDePantallas">Pantallas</label>
        </div>

    </div>

    <div class="center-align col-sm-12">
        <button type="submit" title="Aceptar" class="btn-floating btn-sm green" style="border-radius: 50%" onclick="hidePage()">
            <i class="large material-icons">check</i>
        </button>

